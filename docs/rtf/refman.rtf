   63                     \}\par
   64                     else if ( this->_input_down ) \{\par
   65                         adjust_y += strength;\par
   66                     \}\par
   67                     adjustment_rotated = Vector::rotate_velocity(adjust_x, adjust_y, this->get_rotation());\par
   68                     adjust_x = adjustment_rotated.first;\par
   69                     adjust_y = adjustment_rotated.second;\par
   70                     this->_vector->adjust(adjust_x, adjust_y, 0);\par
   71                     \par
   72                     //\par
   73                     if ( this->_input_left ) \{\par
   74                         this->_vector->adjust(0, 0, -strength);\par
   75                     \}\par
   76                     else if ( this->_input_right ) \{\par
